pub fn rec_fact(n: int) -> int {
    if n == 0 {
        return 1;
    }
    return n * rec_fact(n - 1);
}

pub fn fact(n: int) -> int {
    let i = 1;
    let result = 1;
    while i <= n {
        result = result * i;
        i = i + 1;
    }
    return result;
}